{
  "name": "vue-prosemirror-2",
  "version": "0.3.0",
  "description": "Plugin to integrate prosemirror with Vue 2",
  "author": "Robert Jefe Lindstaedt <robert.lindstaedt@gmail.com>",
  "licence": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eljefedelrodeodeljefe/vue-prosemirror-2.git"
  },
  "homepage": "https://eljefedelrodeodeljefe.github.com/vue-prosemirror-2",
  "main": "./index.js",
  "scripts": {
    "dev:watchify": "watchify -vd -g envify -p browserify-hmr -e examples/browserify/main.js -o dist/build.js",
    "dev:serve": "cd dist && http-server -o -c 1 -a localhost",
    "dev": "npm-run-all --parallel dev:watchify dev:serve",
    "lint": "eslint index.js",
    "test": "karma start karma.conf.js",
    "deploy": "npm run build && gh-pages -d dist",
    "build": "NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o dist/build.css ] -e examples/browserify/main.js | uglifyjs -c warnings=false -m > dist/build.js &&  postcss -c postcss.json",
    "postversion": "TAG=$(echo $(git describe --tags $(git rev-list --tags --max-count=1))) && git tag -a $TAG $TAG -f -m \"$(git log `git describe --tags --abbrev=0 HEAD^`..HEAD --oneline)\" && git push && git push --tags && npm run deploy"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "brfs"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  },
  "dependencies": {
    "insert-css": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "brfs": "^1.4.3",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.1",
    "cross-env": "^2.0.0",
    "element-ui": "^1.0.6",
    "envify": "^3.4.1",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "gh-pages": "^0.12.0",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "normalize.css": "^5.0.0",
    "npm-run-all": "^2.3.0",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-cli": "^2.6.0",
    "postcss-cssnext": "^2.9.0",
    "prosemirror-example-setup": "^0.15.0",
    "prosemirror-markdown": "^0.15.0",
    "prosemirror-menu": "^0.15.0",
    "prosemirror-state": "^0.15.0",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vue": "^2.0.1",
    "vue-ua": "^1.1.2",
    "vueify": "^9.0.0",
    "watchify": "^3.4.0"
  }
}
